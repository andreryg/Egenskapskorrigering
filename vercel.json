{
    "builds": [
        {
            "src": "index.py",
            "use": "@ardnt/vercel-python-wsgi",
            "config": { "maxLambdaSize": "15mb", "wsgiApplicationName": "app", "runtime": "python3.9" }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}
