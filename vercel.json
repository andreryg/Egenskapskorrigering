{
    "builds": [
        {
            "src": "api/app.py",
            "use": "@vercel/python",
            "config": {
                "includeFiles": ["api/createJSON.py"]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "api/app.py"
        }
    ]
}
